<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="scenePane" prefHeight="622.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.will_hero.MainMenuController">
   <children>
      <Pane layoutX="38.0" layoutY="38.0" prefHeight="546.0" prefWidth="275.0">
         <children>
            <Button fx:id="newGameButton" layoutX="64.0" layoutY="162.0" mnemonicParsing="false" onAction="#newGame" prefHeight="52.0" prefWidth="153.0" text="NEW GAME">
               <font>
                  <Font name="Arial" size="13.0" />
               </font>
            </Button>
            <Button fx:id="exitGameButton" layoutX="114.0" layoutY="329.0" mnemonicParsing="false" onAction="#exitGame" text="EXIT">
               <font>
                  <Font name="Arial" size="13.0" />
               </font>
            </Button>
            <Button fx:id="loadGameButton" layoutX="86.0" layoutY="248.0" mnemonicParsing="false" onAction="#loadGame" prefHeight="50.0" prefWidth="109.0" text="LOAD GAME">
               <font>
                  <Font name="Arial" size="13.0" />
               </font>
            </Button>
            <Label layoutX="105.0" layoutY="49.0" text="WILL HERO">
               <font>
                  <Font name="Arial" size="13.0" />
               </font></Label>
         </children>
      </Pane>
   </children>
</AnchorPane>
